{
    "manifest_version": 3,
    "name": "Weather-App",
    "version": "1.0",
    "action": {
        "default_title": "Weather Extension",
        "default_popup": "popup.html",
        "default_icon" : "Images/sun.png"
    },
    "icons": {
"128":"Images/sun.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "geolocation"
    ]
}